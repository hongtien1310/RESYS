
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta charset="utf-8" />
		<title>Login</title>

		<meta name="description" content="User login page" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

		<!-- bootstrap & fontawesome -->
		<link rel="stylesheet" href="~/Areas/admin/content/Css/bootstrap.min.css" />
        <link href="~/Areas/admin/content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

		<!-- text fonts -->
		<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" />
            <script src="~/Scripts/ie-emulation-modes-warning.js"></script>
		<!-- ace styles -->
		<link rel="stylesheet" href="~/Areas/admin/content/Css/ace.min.css" />
		
		<link rel="stylesheet" href="~/Areas/admin/content/Css/ace-rtl.min.css" />
		
	</head>

	<body class="login-layout light-login">
		<div class="main-container">
			<div class="main-content">
				<div class="row">
					<div class="col-sm-10 col-sm-offset-1">
						<div class="login-container">
							<div class="center">
								<h1>
									<span class="red">idoc</span><span class="blue">Net</span>
									<span class="grey" id="id-text2">Application</span>
								</h1>
								<h4 class="blue" id="id-company-text">&copy; idocNet</h4>
							</div>

							<div class="space-6"></div>
                            
                            <section id="loginForm">
    

<div class="position-relative">
								<div id="login-box" class="login-box visible widget-box no-border">
									<div class="widget-body">
										<div class="widget-main">
											<h4 class="header blue lighter bigger">
												<i class="ace-icon fa fa-coffee green"></i>
												Please Enter Your Information
											</h4>
											@RenderBody()

										</div><!-- /.widget-main -->

									</div><!-- /.widget-body -->
								</div><!-- /.login-box -->

								
							</div>
    </section>
							<!-- /.position-relative -->

						</div>
					</div><!-- /.col -->
				</div><!-- /.row -->
			</div><!-- /.main-content -->
		</div><!-- /.main-container -->

		
	</body>
    <script src="~/Scripts/ie10-viewport-bug-workaround.js"></script>
    @RenderSection("scripts", required: false)
</html>
